{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "lib": ["dom", "es2019"],
    "module": "ESNext",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "target": "es2018",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
